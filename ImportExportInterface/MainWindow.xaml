<Window x:Name="MainWindow1" x:Class="ImportExportInterface.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImportExportInterface"
        mc:Ignorable="d"
        Title="Load order tool" Height="100" Width="270" ResizeMode="NoResize">

    <Grid x:Name="MainGrid" Background="#99585858" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Grid.ColumnDefinitions>

        </Grid.ColumnDefinitions>
        <Button x:Name="ButtonExport" Content="Export" HorizontalAlignment="Left" VerticalAlignment="Top" Background="#33DDDDDD" Height="30" Width="120" Click="ButtonExport_Click" FontSize="14" Margin="5,19,0,0"/>
        <Button x:Name="ButtonImport" Content="Import" HorizontalAlignment="Left" Margin="130,19,0,0" VerticalAlignment="Top" Background="#33DDDDDD" Height="30" Width="120" Click="ButtonExport_Click" FontSize="14"/>

    </Grid>

</Window>